{
    "name": "hexlet/code",
    "type": "project",
    "description": "PHP brain-games project",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": "src/",
            "BrainGames\\": "src/"
        },
        "files": [
            "src/Cli.php",
            "src/Even.php",
            "src/Engine.php",
            "src/Games/Calc.php",
            "src/Games/Gcd.php",
            "src/Games/Progression.php",
            "src/Games/Prime.php"
        ]
    },
    "authors": [
        {
            "name": "gilmidenov"
        }
    ],
    "require": {
        "wp-cli/php-cli-tools": "^0.12.1"
    },
    "bin": [
        "bin/brain-games",
        "bin/brain-even",
        "bin/brain-calc",
        "bin/brain-gcd",
        "bin/brain-progression",
        "bin/brain-prime"
    ],
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.11"
    }
}
